{
  "name": "kuroko-portfolio",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "server": "npx ts-node server/src/index.ts",
    "server:dev": "nodemon --watch server --ext ts,json --exec \"npx ts-node server/src/index.ts\"",
